<jasperReport name = "jasperform">
    <parameter name="title" class="java.lang.String"/>
    <field name="location" class="java.lang.String"/>
    <field name="hourRange" class="java.lang.String"/>
    <field name="count" class="java.lang.Integer"/>
    <title>
        <band height="40">
            <textField>
                <reportElement x="0" y="0" width="600" height="40"/>
                <textElement textAlignment = "Center">
                    <font fontName = "SansSerif" size = "22" isBold = "true"/>
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$P{title}]]>
                </textFieldExpression>
            </textField>
        </band>
    </title>

    <columnHeader>
        <band height = "30">
            <staticText>
                <reportElement mode = "Opaque" x = "50" y = "0" width = "500" height = "30" backcolor = "#70A9A9" />
                <box>
                    <topPen lineWidth = "1.5" lineColor = "#000000" />
                    <leftPen lineWidth = "1.5" lineColor = "#000000" />
                    <bottomPen lineWidth = "1.5" lineColor = "#000000" />
                    <rightPen lineWidth = "1.5" lineColor = "#000000" />
                </box>
                <textElement>
                    <font fontName = "SansSerif"/>
                </textElement>

                <text>
                    <![CDATA[]]>
                </text>
            </staticText>
            <staticText>
                <reportElement x = "50" y = "5" width = "200" height = "20" />
                <box>
                    <rightPen lineWidth = "1.5" lineColor = "#000000" />
                </box>

                <textElement textAlignment = "Center" verticalAlignment = "Middle">
                    <font fontName = "SansSerif" isBold = "true" />
                </textElement>
                <text><![CDATA[LOCATION]]></text>
            </staticText>

            <staticText>
                <reportElement x = "250" y = "5" width = "100" height = "20" />
                <box>
                    <rightPen lineWidth = "1.5" lineColor = "#000000" />
                </box>
                <textElement textAlignment = "Center" verticalAlignment = "Middle">
                    <font fontName = "SansSerif" isBold = "true" />
                </textElement>
                <text><![CDATA[COUNT]]></text>
            </staticText>

            <staticText>
                <reportElement x = "350" y = "5" width = "200" height = "20" />

                <textElement textAlignment = "Center" verticalAlignment = "Middle">
                    <font fontName = "SansSerif" isBold = "true" />
                </textElement>

                <text><![CDATA[HOUR-RANGE]]></text>
            </staticText>

        </band>
    </columnHeader>


    <detail>
        <band height="30">
            <staticText>
                <reportElement mode = "Opaque" x = "50" y = "0" width = "500" height = "30" backcolor = "#D3D3D3" />
                <box>
                    <topPen lineWidth = "1.5" lineColor = "#000000" />
                    <leftPen lineWidth = "1.5" lineColor = "#000000" />
                    <bottomPen lineWidth = "1.5" lineColor = "#000000" />
                    <rightPen lineWidth = "1.5" lineColor = "#000000" />
                </box>
                <textElement>
                    <font fontName = "SansSerif"/>
                </textElement>
                <text>
                    <![CDATA[]]>
                </text>
            </staticText>
            <textField>
                <reportElement x="120" y="8" width="160" height="22"/>
                <textElement>
                    <font fontName = "SansSerif"/>
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$F{location}]]>
                </textFieldExpression>
            </textField>
            <textField>
                <reportElement x="280" y="8" width="120" height="22"/>
                <textElement>
                    <font fontName = "SansSerif"/>
                </textElement>
                <textFieldExpression class="java.lang.Integer">
                    <![CDATA[$F{count}]]>
                </textFieldExpression>
            </textField>
            <textField>
                <reportElement x="420" y="8" width="180" height="22"/>
                <textElement>
                    <font fontName = "SansSerif"/>
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$F{hourRange}]]>
                </textFieldExpression>
            </textField>
        </band>
    </detail>
</jasperReport>